{
  "$schema": "https://platformatic.dev/schemas/v1.39.0/composer",
  "composer": {
    "services": [
      {
        "id": "urls",
        "openapi": {
          "url": "/documentation/json",
          "prefix": "/urls"
        }
      },
      {
        "id": "qr",
        "openapi": {
          "url": "/documentation/json",
          "prefix": "/qr"
        }
      }
    ],
    "refreshTimeout": 1000
  },
  "watch": true,
  "plugins": {
    "paths": [
      "./routes"
    ],
    "typescript": "{PLT_COMPOSER_TYPESCRIPT}"
  }
}